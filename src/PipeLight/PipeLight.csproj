<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>False</GeneratePackageOnBuild>
    <Title>PipeLight</Title>
    <PackageId>PipeLight</PackageId>
    <Product>PipeLight</Product>
    <Authors>Asriel Bukharov</Authors>
    <Description>A simple library to help you build pipelines</Description>
    <RepositoryUrl>https://github.com/Ralsei25/PipeLight</RepositoryUrl>
	<PackageLicenseUrl>https://github.com/Ralsei25/PipeLight/blob/master/LICENSE.txt</PackageLicenseUrl>
    <PackageTags>pipelight; pipeline</PackageTags>
    <Copyright>Copyright Â© Asriel Bukharov</Copyright>
  </PropertyGroup>

</Project>
